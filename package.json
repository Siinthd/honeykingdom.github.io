  "private": true,
  "homepage": "https://honeykingdom.github.io",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-ga": "^2.7.0",
    "react-scripts": "^3.2.0",
    "styled-components": "^5.0.0-rc.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "gh-pages": "^2.1.1"
  },
  "scripts": {
